{
    "version": 2,
    "builds": [
        {
            "src": "api/completions.py",
            "use": "@vercel/python"
        },
        {
            "src": "frontend/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "api/completions.py"
        },
        {
            "source": "/google2256de06fae5cd8e.html",
            "destination": "frontend/google2256de06fae5cd8e.html"
        },
        {
            "source": "/(learn|charts|login|signup)",
            "destination": "frontend/index.html"
        },
        {
            "source": "/(.*)",
            "destination": "frontend/$1"
        }
    ]
}